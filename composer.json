{
  "name"        : "icaruspanel/techne",
  "type"        : "library",
  "require"     : {
    "php"                      : "^8.5",
    "ext-pdo"                  : "*",
    "nesbot/carbon"            : "^3.1",
    "justinrainbow/json-schema": "^6.7",
    "composer-plugin-api"      : "^2.0",
    "vlucas/phpdotenv"         : "^5.6",
    "filp/whoops"              : "^2.18",
    "nikic/fast-route"         : "^1.3",
    "monolog/monolog"          : "^3.10",
    "symfony/uid"              : "^8.0"
  },
  "require-dev" : {
    "phpunit/phpunit"              : "^13.0",
    "phpstan/phpstan"              : "^2.1",
    "readalizer/readalizer"        : "^1.0",
    "composer/composer"            : "^2.0",
    "jetbrains/phpstorm-attributes": "^1.2"
  },
  "autoload"    : {
    "psr-4": {
      "Engine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config"      : {
    "allow-plugins": {}
  },
  "scripts"     : {
    "test"   : "@php vendor/bin/phpunit",
    "analyze": "@php vendor/bin/phpstan analyse src tests --memory-limit=2G"
  }
}
